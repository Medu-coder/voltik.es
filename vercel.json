{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/(.*)\\.(js|css|svg|png|jpg|jpeg|gif|webp|ico|txt|xml|json|webmanifest|woff2?)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}