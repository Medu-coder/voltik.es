{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/(.*)\\.(js|css|svg|png|jpg|jpeg|gif|webp|ico|txt|xml|json|webmanifest|woff2?)", "headers": { "cache-control": "public, max-age=31536000, immutable" }, "dest": "/$1.$2" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}